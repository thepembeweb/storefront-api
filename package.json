{
  "name": "storefront-api",
  "version": "1.0.0",
  "description": "Storefront Api with Node.js, Typescript and Postgres ",
  "main": "dist/index.js",
  "scripts": {
    "build": "rimraf dist && tsc",
    "preserve": "npm run build",
    "serve": "cross-env NODE_ENV=development concurrently \"tsc --watch\" \"nodemon -q dist/index.js\"",
    "prestart": "npm run build",
    "start": "cross-env NODE_ENV=production node dist/index.js",
    "jasmine": "jasmine",
    "pretest": "npm-run-all test:db-up build",
    "test": "npm-run-all test:test test:test-cleanup --continue-on-error",
    "test:test": "cross-env NODE_ENV=test npm run jasmine",
    "test:test-cleanup": "npm run test:db-down",
    "test:db-reset": "db-migrate reset --env test",
    "test:db-up": "db-migrate up --env test",
    "test:db-down": "db-migrate down --env test -c 4",
    "lint": "eslint . --ext .ts",
    "lint-and-fix": "eslint . --ext .ts --fix",
    "format": "prettier --config .prettierrc \"src/**/*.ts\" --write",
    "migrate:up": "db-migrate up --env development",
    "migrate:down": "db-migrate down --env development -c 4",
    "migrate:reset": "db-migrate reset --env development"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/body-parser": "^1.19.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jasmine": "^3.10.2",
    "@types/jsonwebtoken": "^8.5.6",
    "@types/node": "^17.0.1",
    "@types/pg": "^8.6.2",
    "@types/supertest": "^2.0.11",
    "@types/typescript": "^2.0.0",
    "@typescript-eslint/eslint-plugin": "^5.8.0",
    "@typescript-eslint/parser": "^5.8.0",
    "body-parser": "^1.19.1",
    "concurrently": "^6.5.1",
    "copyfiles": "^2.4.1",
    "cross-env": "^7.0.3",
    "db-migrate": "^0.11.13",
    "db-migrate-pg": "^1.2.2",
    "dotenv": "^10.0.0",
    "eslint": "^8.5.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "helmet": "^4.6.0",
    "jasmine": "^3.10.0",
    "jasmine-spec-reporter": "^7.0.0",
    "nodemon": "^2.0.15",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "supertest": "^6.1.6",
    "typescript": "^4.5.4"
  },
  "dependencies": {
    "bcrypt": "^5.0.1",
    "cors": "^2.8.5",
    "express": "^4.17.2",
    "jsonwebtoken": "^8.5.1",
    "pg": "^8.7.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thepembeweb/storefront-api.git"
  },
  "keywords": [],
  "author": "Pemberai Sweto",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/thepembeweb/storefront-api/issues"
  },
  "homepage": "https://github.com/thepembeweb/storefront-api#readme"
}
